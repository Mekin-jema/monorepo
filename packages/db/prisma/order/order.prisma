generator Client {
  provider = "prisma-client-js"
  output   = "../../generated/order"
}

datasource db {
  provider = "sqlite"
  url      =  "file:./dev.db"
}

model Cache {
  id        String   @id @default(cuid()) 
  key       String   @unique
  value     String
  expiresAt DateTime?
  createdAt DateTime @default(now())
}
